<head>
<title><%= @title %></title>
<meta name="viewport" content="width=device-width, initial-scale=1.0"></meta>
<%= stylesheet_link_tag    "application", :media => "all" %>
<link href='http://fonts.googleapis.com/css?family=Anton' rel='stylesheet' type='text/css'></link>
<%= stylesheet_link_tag APP_CONFIG['cdn_base_url'] + "/bootstrap/css/bootstrap.min.css" %>
<%= stylesheet_link_tag APP_CONFIG['cdn_base_url'] + "/bootstrap/css/bootstrap-responsive.min.css" %>
<%= stylesheet_link_tag APP_CONFIG['cdn_base_url'] + "/css/base.css" %>
<%= stylesheet_link_tag APP_CONFIG['cdn_base_url'] + "/css/app.css" %>
<%= javascript_include_tag "application" %>
<%= javascript_include_tag APP_CONFIG['cdn_base_url'] + "/jquery/js/jquery.min.js" %>
<%= javascript_include_tag APP_CONFIG['cdn_base_url'] + "/bootstrap/js/bootstrap.min.js" %>
<%= csrf_meta_tags %>
<%= yield(:head) %>
</head>
